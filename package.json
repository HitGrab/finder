{
    "name": "@hitgrab/finder",
    "version": "0.0.21-alpha",
    "description": "Headless datatables for things that aren't tables.",
    "type": "module",
    "files": [
        "dist/**/*"
    ],
    "main": "./dist/index.umd.cjs",
    "types": "./dist/index.d.ts",
    "module": "./dist/index.js",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.umd.cjs"
            }
        },
        "./package.json": "./package.json"
    },
    "scripts": {
        "dev": "vite",
        "build": "vite build && tsc --project tsconfig.production.json && cp ./src/types.ts ./dist/types.d.ts",
        "test": "vitest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/HitGrab/finder.git"
    },
    "keywords": [
        "datatable",
        "filter",
        "search",
        "sort",
        "group"
    ],
    "author": "HitGrab Inc.",
    "license": "ISC",
    "sideEffects": false,
    "bugs": {
        "url": "https://github.com/HitGrab/finder/issues"
    },
    "homepage": "https://github.com/HitGrab/finder#readme",
    "dependencies": {
        "lodash": "^4.17.21",
        "react": "^19.1.0",
        "react-dom": "^19.1.0"
    },
    "devDependencies": {
        "@types/lodash": "^4.17.16",
        "@types/node": "^22.15.17",
        "@types/react": "^19.1.3",
        "@types/react-dom": "^19.1.3",
        "@vitejs/plugin-react": "^4.3.2",
        "eslint": "^8.57.1",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-plugin-import": "^2.27.5",
        "prettier": "^3.5.3",
        "typescript": "^5.8.2",
        "vite": "^6.3.5",
        "vite-plugin-checker": "^0.9.0",
        "vitest": "^3.1.3"
    }
}
