{
    "name": "finder",
    "version": "0.0.1",
    "description": "Headless datatables for things that aren't tables.",
    "main": "dist/index.js",
    "type": "module",
    "types": "dist/index.d.ts",
    "files": [
        "/dist"
    ],
    "compile": true,
